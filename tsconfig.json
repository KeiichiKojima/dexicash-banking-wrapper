{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dists",
    "baseUrl": "src",
    "paths": {
      "@services/*": ["services/*"],
      "@database/*": ["database/*"],
      "@routers/*": ["routers/*"],
      "@rabbit/*": ["rabbit/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modues"],
  "references": [ { "path": "src", "prepend": true } ]
}
