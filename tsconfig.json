{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dists",
    "baseUrl": "src",
    "paths": {
      "@database/*": ["database/*"],
      "@routers/*": ["routers/*"]
    }
  },
  "include": ["src/**/*"]
}
