{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "@services/*": ["services/*"],
      "@database/*": ["database/*"],
      "@routers/*": ["routers/*"],
      "@rabbit/*": ["rabbit/*"],
      "@domain/*": ["domain/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modues"]
}
